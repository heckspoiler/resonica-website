{
  "format": "page",
  "id": "release",
  "label": "Release",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID"
        }
      },
      "release_title": {
        "type": "StructuredText",
        "config": {
          "label": "Release Title",
          "placeholder": "Resonica001",
          "allowTargetBlank": true,
          "multi": "heading2"
        }
      },
      "release_date": {
        "type": "StructuredText",
        "config": {
          "label": "Release Date",
          "placeholder": "14.04.2025 (\"Release Date\" is hardcoded)",
          "allowTargetBlank": true,
          "multi": "heading3"
        }
      },
      "release_description": {
        "type": "StructuredText",
        "config": {
          "label": "Release Description",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph"
        }
      },
      "release_image": {
        "type": "Image",
        "config": {
          "label": "Release Image",
          "constraint": {},
          "thumbnails": []
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {}
        }
      }
    },
    "SEO & Metadata": {
      "meta_title": {
        "config": {
          "label": "Meta Title",
          "placeholder": "A title of the page used for social media and search engines"
        },
        "type": "Text"
      },
      "meta_description": {
        "config": {
          "label": "Meta Description",
          "placeholder": "A brief summary of the page"
        },
        "type": "Text"
      },
      "meta_image": {
        "config": {
          "constraint": {
            "height": 1260,
            "width": 2400
          },
          "label": "Meta Image",
          "thumbnails": []
        },
        "type": "Image"
      }
    }
  }
}
